{
    "version": 2,
    "builds": [
      { "src": "public/*", "use": "@vercel/static" },
      { "src": "api/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/public/$1" },    
      { "src": "/v1/chat/completions", "dest": "/api/server.js" } 
    ]
  }
  